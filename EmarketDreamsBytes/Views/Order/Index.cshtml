@model IEnumerable<EmarketDreamsBytes.Entity.Order>

@{
    ViewBag.Title = "Orders";
}

<h3>Orders</h3>


<table class="table">
    <tr>
        <th>
            Order no
        </th>
        <th>
            User Name
        </th>
        <th>
            Total Price
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.OrderId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalPrice)
        </td>
        <td>
            | @Html.ActionLink("Order Detail", "Detail", new { id = item.OrderId }) |

        </td>
    </tr>
}

</table>
